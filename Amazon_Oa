#include <bits/stdc++.h>
using namespace std;

int main() {

    string s;
    cin >> s;
    int n = s.size();

    string t = s;
    sort(t.begin(), t.end());

    //  Case 0 
    if (s == t) {
        cout << 0;
        return 0;
    }

    // Case 1 : sort (0..n-2) 
    {
        string a = s.substr(0, n - 1);
        sort(a.begin(), a.end());
        if (a + s[n - 1] == t) {
            cout << 1;
            return 0;
        }
    }

    // Case 1 : sort (1..n-1)
    {
        string b = s.substr(1);
        sort(b.begin(), b.end());
        if (s[0] + b == t) {
            cout << 1;
            return 0;
        }
    }

    //  Case 2a : (0..n-2) then (1..n-1)
    {
        string a = s.substr(0, n - 1);
        sort(a.begin(), a.end());
        string temp = a + s[n - 1];

        string b = temp.substr(1);
        sort(b.begin(), b.end());

        if (temp[0] + b == t) {
            cout << 2;
            return 0;
        }
    }

    // Case 2b : (1..n-1) then (0..n-2) 
    {
        string b = s.substr(1);
        sort(b.begin(), b.end());
        string temp = s[0] + b;

        string a = temp.substr(0, n - 1);
        sort(a.begin(), a.end());

        if (a + temp[n - 1] == t) {
            cout << 2;
            return 0;
        }
    }

    //  Case 3
    cout << 3;
    return 0;
}
